{
    "roots": [
        "<rootDir>/resources/ts"
    ],
    "testMatch": [
        "**/__tests__/**/*.+(ts|tsx|js)",
        "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
        "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": ["<rootDir>/resources/ts/mock/jest.setup.ts"]
}
